#include <stdio.h>

int main()
{
    int a[20], n, key, i;

    scanf("%d", &n);
    for(i = 0; i < n; i++)
        scanf("%d", &a[i]);

    scanf("%d", &key);

    for(i = 0; i < n; i++)
        if(a[i] == key)
        {
            printf("Element found at position %d", i+1);
            return 0;
        }

    printf("Element not found");
    return 0;
}


input:
5
10 20 30 40 50
30
output:Element found at position 3
